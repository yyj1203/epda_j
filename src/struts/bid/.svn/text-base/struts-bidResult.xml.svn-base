<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
    <package name="bidResult" namespace="/bidResult" extends="struts-default">
    	<!-- 입찰결과을 조회한다. 페이징처리없음 -->
    	<action name="listBidResult" method="listBidResult" class="com.kps.bid.action.BidResultAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 입찰결과을 조회한다. 페이징처리 -->
    	<action name="listBidResultPaging" method="listBidResultPaging" class="com.kps.bid.action.BidResultAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 입찰결과에 글을 등록한다. -->
    	<action name="insertBidResult" method="insertBidResult" class="com.kps.bid.action.BidResultAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 입찰결과을 수정한다. -->
    	<action name="updateBidResult" method="updateBidResult" class="com.kps.bid.action.BidResultAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 입찰결과을 삭제한다. -->
    	<action name="deleteBidResult" method="deleteBidResult" class="com.kps.bid.action.BidResultAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 입찰결과을 읽는다. -->
    	<action name="readBidResult" method="readBidResult" class="com.kps.bid.action.BidResultAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    </package>
</struts> 
