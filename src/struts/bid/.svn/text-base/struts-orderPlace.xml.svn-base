<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
    <package name="orderPlace" namespace="/orderPlace" extends="struts-default">
    	<!-- 발주처을 조회한다. 페이징처리없음 -->
    	<action name="listOrderPlace" method="listOrderPlace" class="com.kps.bid.action.OrderPlaceAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 발주처을 조회한다. 페이징처리 -->
    	<action name="listOrderPlacePaging" method="listOrderPlacePaging" class="com.kps.bid.action.OrderPlaceAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 발주처에 글을 등록한다. -->
    	<action name="insertOrderPlace" method="insertOrderPlace" class="com.kps.bid.action.OrderPlaceAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 발주처을 수정한다. -->
    	<action name="updateOrderPlace" method="updateOrderPlace" class="com.kps.bid.action.OrderPlaceAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 발주처 호기를 수정한다. -->
    	<action name="updateOrderPart" method="updateOrderPart" class="com.kps.bid.action.OrderPlaceAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 발주처을 삭제한다. -->
    	<action name="deleteOrderPlace" method="deleteOrderPlace" class="com.kps.bid.action.OrderPlaceAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 발주처을 삭제한다. -->
    	<action name="deleteOrderPart" method="deleteOrderPart" class="com.kps.bid.action.OrderPlaceAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 발주처을 읽는다. -->
    	<action name="readOrderPlace" method="readOrderPlace" class="com.kps.bid.action.OrderPlaceAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 발주처목록을 불러온다 -->
    	<action name="loadOrderPlace" method="loadOrderPlace" class="com.kps.bid.action.OrderPlaceAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    	<!-- 발주처 호기목록을 불러온다 -->
    	<action name="loadOrderPart" method="loadOrderPart" class="com.kps.bid.action.OrderPlaceAction">
    		<result name="success" type="stream">
    			<param name="inputName">inputStream</param>
    			<param name="contentType">text/plain</param>
    			<param name="bufferSize">4096</param>
    		</result>
    	</action>
    	
    </package>
</struts> 
