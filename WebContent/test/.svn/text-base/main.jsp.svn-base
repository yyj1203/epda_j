<?xml version="1.0" encoding="EUC-KR" ?>
<%@ page language="java" contentType="text/html; charset=EUC-KR"
    pageEncoding="EUC-KR"%>
<% String root = request.getContextPath(); %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=EUC-KR" />
<title>메인페이지</title>
<link href="./main.css" type="text/css" rel="stylesheet" />
</head>
<body>
	<!-- 모든것을 감싸는 div -->
	<div id="all">
		<!-- 헤더 -->
		<div id="top">
			<img src="<%=root %>/img/test_top.jpg" alt="헤더이미지" />
		</div>
		
		<!-- 왼쪽 메뉴 -->
		<div id="left">
			<ul>
				<li><a href="http://www.itman.co.kr" title="aaa링크" target="content" >aaa</a></li>
				<li><a href="http://www.kps.co.kr" title="bbb링크" target="content" >bbb</a></li>
				<li><a href="http://www.naver.com" title="ccc링크" target="content" >ccc</a></li>
				<li><a href="http://www.daum.net" title="ddd링크" target="content" >ddd</a></li>
				<li><a href="http://www.google.co.kr" title="eee링크" target="content" >eee</a></li>
			</ul>
		</div>
		
		<!-- 컨텐츠 내용 -->
		<div id="right">
			<iframe src="http://www.itman.co.kr" name="content" scrolling="auto"
			frameborder="0"></iframe>
		</div>
		
		<!-- 풋터 -->
		<div id="bottom">
		</div>
	</div>

</body>
</html>